  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#9098b0", "fontStyle": "italic" }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": { "foreground": "#a4e09c" }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": { "foreground": "#f8b080" }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": { "foreground": "#d0d6f0" }
    },
    {
      "scope": ["variable.language", "variable.other.constant"],
      "settings": { "foreground": "#f08898" }
    },
    {
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#e69ca4", "fontStyle": "italic" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#c8a2f4" }
    },
    {
      "scope": ["keyword.control"],
      "settings": { "foreground": "#c8a2f4" }
    },
    {
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#84d8e8" }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": { "foreground": "#f8b080" }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": { "foreground": "#f5dea4" }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#84b4f8" }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#f5dea4" }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": { "foreground": "#90dcd0" }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": { "foreground": "#9098b0" }
    },
    {
      "scope": ["support.constant", "constant.other"],
      "settings": { "foreground": "#f8b080" }
    },
    {
      "scope": ["meta.decorator", "entity.name.decorator"],
      "settings": { "foreground": "#f8b080" }
    },
    {
      "scope": ["string.regexp"],
      "settings": { "foreground": "#f2c0e4" }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": { "foreground": "#f2c0e4" }
    },
    {
      "scope": ["markup.heading"],
      "settings": { "foreground": "#f8b080", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.bold"],
      "settings": { "foreground": "#f08898", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic"],
      "settings": { "foreground": "#f08898", "fontStyle": "italic" }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": { "foreground": "#a4e09c" }
    },
    {
      "scope": ["markup.quote"],
      "settings": { "foreground": "#f5dea4", "fontStyle": "italic" }
    },
    {
      "scope": ["markup.list"],
      "settings": { "foreground": "#90dcd0" }
    },
    {
      "scope": ["markup.inserted"],
      "settings": { "foreground": "#a4e09c" }
    },
    {
      "scope": ["markup.deleted"],
      "settings": { "foreground": "#f08898" }
    },
    {
      "scope": ["markup.changed"],
      "settings": { "foreground": "#f5dea4" }
    },
    {
      "scope": ["meta.diff.header"],
      "settings": { "foreground": "#84b4f8" }
    }
  ]
}
